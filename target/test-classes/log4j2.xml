<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">


	<Appenders>

		<File name="File" fileName="logs/output.log">
			<PatternLayout
				pattern="%d{dd-MM-yyyy HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
		</File>

	</Appenders>

	<Loggers>
		<Root level="trace">
			<AppenderRef ref="File" />
		</Root>
	</Loggers>
</Configuration>
